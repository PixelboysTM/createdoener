{
  "multipart": [
    {
      "when": {
        "fill": "full",
        "state": "cooked"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_full"
      }
    },
    {
      "when": {
        "fill": "two",
        "state": "cooked"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_two"
      }
    },
    {
      "when": {
        "fill": "one",
        "state": "cooked"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_one"
      }
    },
    {
      "when": {
        "fill": "full",
        "state": "medium"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_full_medium"
      }
    },
    {
      "when": {
        "fill": "two",
        "state": "medium"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_two_medium"
      }
    },
    {
      "when": {
        "fill": "one",
        "state": "medium"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_one_medium"
      }
    },
    {
      "when": {
        "fill": "full",
        "state": "raw"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_full_raw"
      }
    },
    {
      "when": {
        "fill": "two",
        "state": "raw"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_two_raw"
      }
    },
    {
      "when": {
        "fill": "one",
        "state": "raw"
      },
      "apply": {
        "model": "createdoener:block/doener_spear_filled_one_raw"
      }
    },
    {
      "when": {
        "dir": "west"
      },
      "apply": {
        "model": "createdoener:block/doener_spear",
        "y": 90
      }
    },
    {
      "when": {
        "dir": "east"
      },
      "apply": {
        "model": "createdoener:block/doener_spear",
        "y": 270
      }
    },
    {
      "when": {
        "dir": "south"
      },
      "apply": {
        "model": "createdoener:block/doener_spear",
        "y": 0
      }
    },
    {
      "when": {
        "dir": "north"
      },
      "apply": {
        "model": "createdoener:block/doener_spear",
        "y": 180
      }
    }
  ]
}
